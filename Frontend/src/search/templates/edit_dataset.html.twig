{% include 'head.html.twig' %}
<div class="ui grid container">
   <div class="row"></div>
   <div class="row">
      <h1> Edit document </h1>
   </div>
   <div class="ui ten wide column centered">
      {% if error %}
      <div class="ui warning message">
         <div class="header">
            Warning!  
         </div>
         {{error}}
      </div>
      {%endif%}
      <form class="ui form" action="editrecord?id={{doi}}" method="post" enctype="multipart/form-data" >
         {% include 'formdataset.html.twig' %}
         <div class="ui grid ">
            <div class="ui sixteen wide column ">
            <div class="ui error message"></div>
            <div class="row"></div>
            <a href="javascript:history.back();" class="ui red button" >Cancel</a>
            <button class="ui submit right floated  button" >Submit</button>
            </div>
            <div class="row"></div>
         </div>

      </form>
   </div>

<script type="text/javascript">
   APP.modules.upload.init();
   
   
</script>
{% include 'footer.html.twig' %}